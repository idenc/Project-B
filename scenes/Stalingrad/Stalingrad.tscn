script/source = "extends Label

func update_objective_text(won = false):
	if won == false:
		var t = \"\"
		for i in get_node(\"../../\").objectives:
			t += \"\\n\" + i
		text = \"Objectives: \" + t
	else:
		text = \"Level Complete!\"
func _ready():
	update_objective_text()

"

